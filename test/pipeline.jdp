pipeline {
     agent any

    stages {
        stage('Pull Stage') {
            steps {
                git 'https://github.com/Sourabh-Ambhore/onlinebookstore_Source_code.git'
                sh '''ls'''
                sh 'pwd'
                echo 'Pull stage succeed'
            }
        }
       stage('Build Stage') {
    agent { label 'node' } // specific to this stage
    steps {
        sh '/opt/apache-maven-3.9.9/bin/mvn clean package'
        echo 'Build Stage succeeded'
    }
}

        stage('Test') {
            steps {
                echo 'TEST'
            }
        }
        stage('Deploy') {
            steps {
                echo 'DEPLOY'
            }
        }
    }
}
